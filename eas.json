{
  "cli": {
    "version": ">= 5.9.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {},
    "apk": {
      "android": {
        "buildType": "apk"
      }
    },
    "optimized-apk": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "withoutCredentials": true,
        "image": "latest",
        "env": {
          "EXPO_USE_HERMES": "1"
        }
      },
      "autoIncrement": true,
      "channel": "optimized"
    }
  }
}
